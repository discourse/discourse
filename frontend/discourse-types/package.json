{
  "name": "@discourse/types",
  "version": "0.0.1",
  "description": "Types only",
  "license": "GPL-2.0-only",
  "author": "Discourse",
  "type": "module",
  "files": [
    "declarations",
    "external-types",
    "external-types.d.ts",
    "package.json",
    "tsconfig-plugin.json"
  ],
  "scripts": {
    "generate": "node generate-dts.js"
  },
  "exports": {
    "./tsconfig-plugin": "./tsconfig-plugin.json",
    "./external-types": {
      "types": "./external-types.d.ts"
    },
    "./tests/*": {
      "types": "./declarations/tests/*.d.ts"
    },
    "./admin/*": {
      "types": "./declarations/admin/*.d.ts"
    },
    "./*": {
      "types": "./declarations/app/*.d.ts"
    }
  },
  "dependencies": {
    "@ember-compat/tracked-built-ins": "^0.9.1",
    "@ember/render-modifiers": "^3.0.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.4.1",
    "@floating-ui/dom": "^1.7.4",
    "@glimmer/component": "^2.0.0",
    "@glimmer/syntax": "0.93.1",
    "@glint/ember-tsc": "^1.0.8",
    "@glint/template": "^1.7.3",
    "@types/qunit": "^2.19.13",
    "@types/rsvp": "^4.0.9",
    "ember-modifier": "^4.2.2",
    "ember-qunit": "^9.0.4",
    "ember-source": "~6.6.0"
  },
  "devDependencies": {
    "compare-versions": "^6.1.1"
  },
  "engines": {
    "node": ">= 18",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": "^10"
  }
}
