{
  "name": "@discourse/types",
  "version": "0.0.2",
  "description": "Types only",
  "license": "GPL-2.0-only",
  "author": "Discourse",
  "type": "module",
  "files": [
    "declarations",
    "external-types",
    "external-types.d.ts",
    "tsconfig-plugin.json"
  ],
  "scripts": {
    "generate": "node generate-dts.js"
  },
  "exports": {
    "./tsconfig-plugin": "./tsconfig-plugin.json",
    "./external-types": {
      "types": "./external-types.d.ts"
    },
    "./tests/*": {
      "types": "./declarations/tests/*.d.ts"
    },
    "./admin/*": {
      "types": "./declarations/admin/*.d.ts"
    },
    "./*": {
      "types": "./declarations/app/*.d.ts"
    }
  },
  "devDependencies": {
    "compare-versions": "^6.1.1"
  },
  "engines": {
    "node": ">= 18",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": "^10"
  }
}
