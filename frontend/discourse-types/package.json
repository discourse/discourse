{
  "name": "@discourse/types",
  "version": "0.0.0",
  "description": "Types only",
  "license": "GPL-2.0-only",
  "author": "Discourse",
  "exports": {
    "./tsconfig-plugin": "./tsconfig-plugin.json",
    "./external-types": {
      "types": "./external-types.d.ts"
    },
    "./tests/*": {
      "types": "./declarations/tests/*.d.ts"
    },
    "./admin/*": {
      "types": "./declarations/admin/*.d.ts"
    },
    "./*": {
      "types": "./declarations/app/*.d.ts"
    }
  },
  "dependencies": {
    "ember-source": "~6.6.0",
    "@types/@glimmer__component": "npm:@glimmer/component@^2.0.0",
    "@types/@glint__ember-tsc": "npm:@glint/ember-tsc@^1.0.7",
    "@types/@glint__template": "npm:@glint/template@^1.7.2"
  },
  "engines": {
    "node": ">= 18",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": "^10"
  }
}
