//= depend_on 'client.ar.yml'
//= require locales/i18n
<%= JsLocaleHelper.output_locale(:ar) %>

I18n.pluralizationRules['ar'] = function (n) {
  if (n == 0) return "zero";
  if (n == 1) return "one";
  if (n == 2) return "two";
  if (n%100 >= 3 && n%100 <= 10) return "few";
  if (n%100 >= 11) return "many";
  return "other";
};
